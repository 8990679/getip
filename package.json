{
  "name": "getip",
  "version": "1.0.0",
  "description": "A lightweight Cloudflare Worker that returns client IPv4/IPv6 as JSON, with extra endpoints and caching.",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev --local --inspect | cat",
    "deploy": "wrangler deploy",
    "format": "prettier --write .",
    "lint": "eslint --max-warnings=0 .",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "ip",
    "ipv4",
    "ipv6",
    "api"
  ],
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250204.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "miniflare": "^3.20250129.3",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "vitest": "^2.1.4",
    "wrangler": "^3.90.0"
  }
}

