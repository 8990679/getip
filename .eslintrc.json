{
  "env": {
    "es2022": true,
    "worker": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:import/recommended",
    "prettier"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "warn",
    "import/no-unresolved": "off"
  }
}

